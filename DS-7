#include <stdio.h>
#define SIZE 5

int cq[SIZE], front = -1, rear = -1;

void enqueue(int val) {
    if ((rear + 1) % SIZE == front)
        printf("Queue Full\n");
    else {
        if (front == -1)
            front = 0;
        rear = (rear + 1) % SIZE;
        cq[rear] = val;
    }
}

void dequeue() {
    if (front == -1)
        printf("Queue Empty\n");
    else {
        printf("Dequeued: %d\n", cq[front]);
        if (front == rear)
            front = rear = -1;
        else
            front = (front + 1) % SIZE;
    }
}

void display() {
    if (front == -1)
        printf("Queue Empty\n");
    else {
        int i = front;
        printf("Queue: ");
        while (1) {
            printf("%d ", cq[i]);
            if (i == rear) break;
            i = (i + 1) % SIZE;
        }
        printf("\n");
    }
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    display();
    dequeue();
    display();
    return 0;
}
